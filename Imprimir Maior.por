programa {
  // Imprimir Maior
  funcao inicio() {
    inteiro num1, num2

    escreva("Insira o primeiro valor para compração: \n")
    leia (num1)

    escreva("Insira o segundo valor para compração: \n")
    leia (num2)
  
    se (num1>num2){
      escreva("O primeiro número é maior.")
    }  
    senao se (num2>num1) {
      escreva ("O segundo número é maior.")
    }
    senao{
      escreva ("São Iguais")
    }
  }
  
}
