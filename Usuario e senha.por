programa {
  funcao inicio() {
    cadeia nome, senha
    
    escreva ("Digite seu usuario \n")
    leia (nome)
    escreva ("Digite sua senha \n")
    leia (senha)

    enquanto(nome==senha){
      escreva ("Digite seu usuario \n")
    leia (nome)
    escreva ("Digite sua senha \n")
    leia (senha)
    }

  }
}
