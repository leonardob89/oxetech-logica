programa {
  // Maior ou menor que 6
  funcao inicio() {
    real nota

    escreva("Insira a sua nota!\n")
    leia (nota)

    se(nota<6){
      escreva("Nota Vermelha")
              }
    senao se (nota ==6){
      escreva("Nota Azul")
                        }
    senao {
      escreva("Nota Verde")
    }                              
  }
}
